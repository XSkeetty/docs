{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "HoundVPN",
  "colors": {
    "primary": "#03BBA7",
    "light": "#03BBA7",
    "dark": "#03BBA7"
  },
  "background": {
    "decoration": "grid",
    "light": "#FFFFFF",
    "dark": "#131313"
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Инструкции",
        "pages": [
          "index",
          {
            "group": "Подключения",
            "pages": [
              "setup/ios",
              "setup/android",
              "setup/macos",
              "setup/windows",
              "setup/apple_tv",
              "setup/android_tv",
              "setup/router"
            ]
          }
        ]
      },
      {
        "tab": "Статьи",
        "pages": [
          "index",
          {
            "group": "Безопасность",
            "pages": [
              "vpn_dangers",
              "vpn_danger1",
              "vpn_dangers2"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/HoundLight.svg",
    "dark": "/logo/HoundDark.png"
  },
  "search": {
    "prompt": "Поиск..."
  },
  "integrations": {
    "telemetry": {
      "enabled": false
    }
  }
}